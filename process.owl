        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies that some thing participates in a process</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/process.owl#requirement -->

