    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#allowed_by -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#allowed_by">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#qualified_by"/>
        <owl:inverseOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#allows"/>
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a qualified Allowed to the norm allowing it</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#allows -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#allows">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#qualifies"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a norm to the thing it allows</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#commanded_by -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#commanded_by">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#allowed_by"/>
        <owl:inverseOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#commands"/>
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Obliged"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#commands -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#commands">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#allows"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Obliged"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#disallowed_by -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#disallowed_by">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#qualified_by"/>
        <owl:inverseOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#disallows"/>
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a qualified Disallowed to the norm disallowing it</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#disallows -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#disallows">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#qualifies"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a norm to the thing it disallows</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#normatively_comparable -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_comparable">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#qualitatively_comparable"/>
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Normatively_Qualified"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Normatively_Qualified"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#normatively_equivalent_or_better -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_equivalent_or_better">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_comparable"/>
        <owl:inverseOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_equivalent_or_worse"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#normatively_equivalent_or_worse -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_equivalent_or_worse">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_comparable"/>
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#normatively_not_equivalent -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_not_equivalent">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_comparable"/>
        <owl:inverseOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_not_equivalent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#normatively_strictly_better -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_strictly_better">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_equivalent_or_better"/>
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_not_equivalent"/>
        <owl:inverseOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_strictly_worse"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Obliged"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#normatively_strictly_worse -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_strictly_worse">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_equivalent_or_worse"/>
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_not_equivalent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Obliged"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed"/>
    </owl:ObjectProperty>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#strictly_equivalent -->

    <owl:ObjectProperty rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#strictly_equivalent">
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_equivalent_or_better"/>
        <rdfs:subPropertyOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_equivalent_or_worse"/>
        <owl:inverseOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#strictly_equivalent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed"/>
        <rdfs:range rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.estrellaproject.org/lkif-core/expression.owl#Document -->

    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/expression.owl#Document">
        <owl:disjointWith rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Custom"/>
    </rdf:Description>
    


    <!-- http://www.estrellaproject.org/lkif-core/expression.owl#Medium -->

    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/expression.owl#Medium">
        <rdfs:comment>媒介是表达的承载者，即外化命题。 命题一旦通过某种媒介外化，就会成为表达。</rdfs:comment>
        <rdfs:label xml:lang="zh-cn">媒介</rdfs:label>
    </rdf:Description>
    


    <!-- http://www.estrellaproject.org/lkif-core/legal-action.owl#Legal_Person -->

    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/legal-action.owl#Legal_Person">
        <rdfs:comment>法律实体是自然人或法律结构，通过该法律实体，法律允许一群自然人为某些目的行事，就像它是一个单一的复合个体一样。最常见的目的是诉讼，财产所有权和合同。有时被称为公司人格或法人资格，这个概念允许通过以法人实体的名义拥有所有权，诉讼和协议而不是组成实体的人的几个名称来轻松开展业务。

法人实体不一定与其组成的自然人不同。大多数法律实体只是简单地合并为方便起见而构成的人。与其成员有独立存在的法人实体称为公司或公司。这种区别赋予公司独特的永久继承特权，通常也是公司成员有限责任的来源。其他一些法律实体也享有成员的有限责任，但不是因为存在单独存在（来源：Wikipedia.org）</rdfs:comment>
    </rdf:Description>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Action_Power -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Action_Power">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Hohfeldian_Power"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An action-power consists in a generic power to produce a legal effect through an action determining it. Also called Subjections</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Allowed -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#allowed_by"/>
                <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Permission"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_equivalent_or_worse"/>
                <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Normatively_Qualified"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Normatively_Qualified"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">See normatively qualified. Alternative labels: permitted, sanctioned, let, licensed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Allowed_And_Disallowed -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed_And_Disallowed">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed"/>
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Something which is both allowed and disallowed through some norm</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Belief_In_Violation -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Belief_In_Violation">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/expression.owl#Belief"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#towards"/>
                        <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#Belief"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A belief in some violation</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Code -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Code">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Document"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#bears"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Norm"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#utterer"/>
                                <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/legal-action.owl#Legislative_Body"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#bears"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#utterer"/>
                        <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/legal-action.owl#Legislative_Body"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A legal code bears one or more norms, all of which are uttered by some legislative body. It cannot bear expressions which are not uttered by a legislative body.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Code_of_Conduct -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Code_of_Conduct">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Document"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Soft_Law"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a code outlining the responsibilities of or best practice for an individual or organisation, such as a set of principles of good corporate behaviour adopted by a business\</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Contract -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Contract">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Document"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#bears"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Norm"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#utterer"/>
                                <owl:someValuesFrom>
                                    <owl:Class>
                                        <owl:unionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/legal-action.owl#Legal_Person"/>
                                            <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/legal-action.owl#Natural_Person"/>
                                        </owl:unionOf>
                                    </owl:Class>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#bears"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#utterer"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/legal-action.owl#Legal_Person"/>
                                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/legal-action.owl#Natural_Person"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A contract bears one or more norms, all of which are uttered by some natural person or legal person. It cannot bear expressions which are uttered by a different kind of agent.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Custom -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Custom">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#Medium"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The collective memory of some group of agents, i.e. propositions beared by custom are shared (i.e. held) by all members of the group</rdfs:comment>
        <rdfs:comment>某些代理人的集体记忆，即由习俗承担的命题，由该集团的所有成员共享（即持有）</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Customary_Law -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Customary_Law">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Custom"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Source"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In law, custom, or customary law consists of established patterns of behaviour that can be objectively verified within a particular social setting.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Declarative_Power -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Declarative_Power">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Potestative_Expression"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A declarative power covers the case when an effect is produced through the party’s declaration of it</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">We say that j has the declarative power to realize A to mean that if j declares A, then it is legally valid that A. For example, if x has the declarative power to terminate y’s obligation toward x to do then if x declares that y’s obligation toward x finishes, then it is legally valid that this obligation finishes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Decree -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Decree">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Document"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Proclamation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The word decree is often used as a derogative term for any authoritarian decision</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Definitional_Expression -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Definitional_Expression">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Expression"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A definition in a legal context (for example “x means y” or “by x it is meant y”);</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Directive -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Directive">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Document"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Proclamation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Examples are European Union directive, a legislative act of the European Union and Directives, used by United States Government agencies (particularly the Department of Defense) to convey policies, responsibilities, and procedures.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#disallowed_by"/>
                <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Prohibition"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_strictly_better"/>
                <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Normatively_Qualified"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">See Qualified. Alternative labels: violation, prohibited, forbidden</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed_Intention -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed_Intention">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/expression.owl#Intention"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#towards"/>
                        <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#Intention"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The propositional content (i.e. that which is intended) of the intention is disallowed. Interesting concept for establishing mens rea (guilty mind), although &apos;disallowed&apos; is a weaker qualification than criminal. Note the distinction between purposely committing a crime and knowingly committing a crime. The crime committed does not have to match the crime intended for establishing intent!</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Enabling_Power -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Enabling_Power">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Potestative_Expression"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An enabling power is used in cases where the law aims at enabling some agent to produce an effect in a particular way.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Evaluative_Expression -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Evaluative_Expression">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#Evaluative_Proposition"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Expression"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A legal evaluative expression asserts that something is good or bad, is a value to be optimised or an evil to be minimised (for example “human dignity is value”, “participation ought to be promoted”);</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Exclusionary_Right -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Exclusionary_Right">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Obligative_Right"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An exclusionary right concerns the prohibition against performing certain inferences (against reasoning in certain ways), or 
against using certain kinds of premises for certain purposes, in the interest of a particular person. This is especially the case with anti-discrimination rules.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Existential_Expression -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Existential_Expression">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Expression"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Establishes or terminates the existence of a legal entity (for example “the company ceases to exist”);</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Hohfeldian_Power -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Hohfeldian_Power">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Potestative_Expression"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A hohfeldian power covers any action which determines a legal effect.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Immunity -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Immunity">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Hohfeldian_Power"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Also called Disabilities</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#International_Agreement -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#International_Agreement">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Source"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An agreement entered into by actors in international law, namely states and international organizations. </rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#International_Arbitration -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#International_Arbitration">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Soft_Law"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">International Arbitration is the established method today for resolving disputes between parties to international commercial agreements. As with arbitration generally, it is a creature of contract, i.e., the parties&apos; decision to submit any disputes to private adjudication by one or more arbitrators appointed in accordance with rules the parties themselves have agreed to adopt, usually by including a provision for the same in their contract. The practice of international arbitration has developed so as to allow parties from different legal and cultural backgrounds to resolve their disputes, generally without the formalities of their underlying legal systems.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Doctrine -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Doctrine">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Source"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Legal doctrine is a framework, set of rules, procedural steps, or test, often established through precedent in the common law, through which judgments can be determined in a given legal cas</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Document -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Document">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#Document"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Source"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A legal document is a document bearing norms or normative statements. By virtue of this definition the norm-as-propositional-attitude is reified as norm-as proposition. In other words, the norm being expressed through the legal source is an expression of the propositional attitude.</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subclasses of legal source can be distinguished through the authority of the agent creating the expression (i.e. the agent holding the norm).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Expression -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Expression">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#Expression"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#attitude"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/process.owl#created_by"/>
                        <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/legal-action.owl#Public_Act"/>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Legal expressions are created by  some legal speech act and qualified by a communicated attitude</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Source -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Source">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#bears"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Expression"/>
                            <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Norm"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#Medium"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A legal source is a source for legal statements, both norms and legal expressions. In a sense it is literally a &apos;source&apos; of law</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Liability_Right -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Liability_Right">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Right"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A liability right expresses some right to act that brings with it some liability to perform another (compensating) act. For instance, if some k performs the permitted action A, then k will have to perform another action B for the benefit of j.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Liberty_Right -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Liberty_Right">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Right"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When, for the benefit of a person, this person is both permitted to perform and to omit an action – that is, when the action is facultative – we can say that he or she has a liberty right with regard to that action.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Mandatory_Precedent -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Mandatory_Precedent">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Precedent"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In law, a binding precedent (also mandatory precedent or binding authority) is a precedent which must be followed by all lower courts. It is usually created by the decision of a higher court, such as the House of Lords in the United Kingdom. Binding precedent relies on the legal principle of stare decisis.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Non-binding_International_Agreement -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Non-binding_International_Agreement">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#International_Agreement"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Soft_Law"/>
        <owl:disjointWith rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Treaty"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Some international agreement that is not binding under international law</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Norm -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Norm">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#qualifies"/>
                <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Normatively_Qualified"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#Qualification"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A norm is a kind of Qualification. A qualification which normatively qualifies some thing (i.e. some normatively qualified): i.e. a qualification which allows or disallows some thing.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Normatively_Qualified -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Normatively_Qualified">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#qualified_by"/>
                <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Norm"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#Qualified"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_comparable"/>
                <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Normatively_Qualified"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Some thing which is qualified (allowed, disallowed) by a norm, i.e. a norm applies to the thing. Taking the principle of deontic choice to mean that the utterer of a normative statement intends to influence choices made by the addressee of the statement, the qualified thing should is comparable to some alternative. Note that Qualified does not partition into Allowed and Disallowed. Firstly, things can be Allowed by one Norm and Disallowed by another one. Secondly, the logical complement of the thing allowed by a permission is qualified (as worse or equal than the thing allowed), but neither allowed nor disallowed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Obligation -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Obligation">
        <owl:equivalentClass rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Prohibition"/>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Permission"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">See prohibition</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Obligative_Right -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Obligative_Right">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Right"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Directed obligative ought-to-do are also called obligative rights. k has the obligative  right that j does A iff it is obligatory, toward k, that j does A. An example of obligative  right is “it is obligatory, toward Mary, that Tom pays 1,000 euro to John”</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Obliged -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Obliged">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#allowed_by"/>
                <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Obligation"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#normatively_strictly_worse"/>
                <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Is allowed by an Obligation, i.e. the alternative is strictly_worse according to the Obligation. Alternative labels: directed, commanded.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Observation_of_Violation -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Observation_of_Violation">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.estrellaproject.org/lkif-core/expression.owl#Observation"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/role.owl#played_by"/>
                        <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/expression.owl#Problem"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An observation of a violation, i.e. of something that is Disallowed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Permission -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Permission">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#allows"/>
                        <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#allows"/>
                        <owl:allValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Allowed"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Norm"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Permission allows propositions, to which therefore the predication Allowed applies. The thing(s) allowed is/are a subset of the thing(s) qualified by the permission. If S then P(A) means that S is qualified and S and A is allowed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Permissive_Right -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Permissive_Right">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Right"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The  negation of a directed obligation is a directed permission. However, it counts as a right,  namely, a permissive right, only when such negation is aimed at benefitting the author of  the permitted action. </rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Persuasive_Precedent -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Persuasive_Precedent">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Precedent"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In law, a persuasive precedent or advisory precedent is a precedent that need not be followed under the legal principle of stare decisis, but is nevertheless followed. Sources of persuasive precedent include the obiter dicta in the judgment of a court whose judgment would otherwise be binding, and the judgments of courts in other jurisdictions where the facts and/or legal system are similar to the case at hand. If a superior court adopts a persuasive precedent, it may become binding in the jurisdiction.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Potestative_Expression -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Potestative_Expression">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Expression"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attributes a power to some agent (for example “a worker has the power to terminate his work contract”);</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Potestative_Right -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Potestative_Right">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Enabling_Power"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A potestative right is an enabling power which is meant to benefit the holder of the power. For example, if some animal y does not 
belong to anybody, then x has the potestative-right to start x’s ownership of the animal,  by capturing y.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Precedent -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Precedent">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Source"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In law, a precedent or authority is a legal case establishing a principle or rule that a court may need to adopt when deciding subsequent cases with similar issues or facts. The term may also refer to the collective body of case law that a court should consider when interpreting the law. When a precedent establishes an important legal principle, or represents new or changed law on a particular issue, that precedent is often known as a landmark decision.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Proclamation -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Proclamation">
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Legal_Source"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A proclamation (Lat. proclamare, to make public by announcement) is an official declaration.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Prohibition -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Prohibition">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#allows"/>
                        <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Obliged"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#disallows"/>
                        <owl:someValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#allows"/>
                        <owl:allValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Obliged"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#disallows"/>
                        <owl:allValuesFrom rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Disallowed"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.estrellaproject.org/lkif-core/norm.owl#Permission"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Prohibition obliges/allows thing(s), to which therefore the predication Obliged applies, and disallows thing(s), to which therefore the predication Disallowed applies. The union of the thing(s) allowed and the thing(s) disallowed is qualified by the prohibition. The things allowed and disallowed by the prohibition are disjoint (and completely partition the space of things qualified). If S then O(A) means that S is qualified, S and A is obliged/allowed, and S and not A is disallowed. If S then F(A) means that S is qualified, S and A is disallowed, and S and not A is obliged/allowed. The difference between Obligation and Prohibition is in which part of the partition of the qualified space is explicitly described by the normative statement. Alternative label: directive</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.estrellaproject.org/lkif-core/norm.owl#Qualificatory_Expression -->

    <owl:Class rdf:about="http://www.estrellaproject.org/lkif-core/norm.owl#Qualificatory_Expression">
